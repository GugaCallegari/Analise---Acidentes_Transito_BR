CREATE DATABASE ACIDENTES_TRANSITO_BR

USE ACIDENTES_TRANSITO_BR

CREATE TABLE ACIDENTES(

ID_ACIDENTE INT NOT NULL PRIMARY KEY,
DATA DATETIME,
DIA_SEMANA VARCHAR(13),
HORARIO DATETIME,
UF CHAR(2),
BR INT,
KM NUMERIC (3,1),
MUNICIPIO VARCHAR(30),
CAUSA VARCHAR (30),
TIPO VARCHAR (30),
CLASSIFICACAO VARCHAR (30),
FASE_DIA VARCHAR(10),
SENTIDO_VIA VARCHAR(11),
CLIMA VARCHAR (15),
TIPO_PISTA VARCHAR (15),
TRACADO_VIA VARCHAR (15),
USO_SOLO CHAR (3),
PESSOAS INT,
MORTOS INT,
ILESOS INT,
INGORADOS INT,
FERIDOS_LEVES INT,
FERIDOS_GRAVES INT,
VEICULOS INT
)
go
